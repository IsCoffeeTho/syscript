{
	"name": "syscript",
	"version": "0.0.1",
	"devDependencies": {
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"private": true,
	"scripts": {
		"start": "bun run src/sysc.ts",
		"build": "bun run build-sysc",
		"build-sysc": "bun build --minify --sourcemap --production --outfile=sysc --keep-names --compile src/sysc.ts"
	}
}
